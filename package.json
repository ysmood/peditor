{
  "name": "peditor",
  "version": "0.0.1",
  "description": "A highly extendable application to abstract and visualize the edit procedure of common web pages.",
  "main": "js/app.js",
  "scripts": {
    "client": "node_modules/.bin/bower install",
    "db": "node_modules/.bin/coffee kit/init_database.coffee",
    "watch": "kit/watch.sh",
    "test": "test/test.sh",
    "start": "kit/server_ctl.sh start",
    "stop": "kit/server_ctl.sh stop",
    "launch": "nohup kit/server_ctl.sh start >/dev/null 2>&1 &"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ysmood/peditor.git"
  },
  "keywords": [
    "webpage",
    "editor"
  ],
  "author": "Yad Smood",
  "license": "BSD-2-Clause",
  "readmeFilename": "README.html",
  "bugs": {
    "url": "https://github.com/ysmood/peditor/issues"
  },
  "dependencies": {
    "bower": "~1.2.4",
    "express": "~3.3.4",
    "request": "~2.25.0",
    "consolidate": "~0.9.1",
    "underscore": "~1.5.1",
    "coffee-script": "~1.6.3",
    "stylus": "~0.36.1",
    "Faker": "~0.5.11"
  },
  "devDependencies": {
    "node-inspector": "*"
  }
}
